  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Edge",
      "request": "launch",
      "type": "pwa-msedge",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/apps/cat-match",
      "pathMappings": [
        {
          "url": "webpack:///src",
          "path": "${webRoot}/src"
        }
      ],
      "preLaunchTask": "npm: start"
    }
  ]
}
